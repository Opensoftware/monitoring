{
  "checks": {
    "load_metrics": {
      "type": "metric",
      "command": "load-metrics.rb",
      "subscribers": ["all"],
      "handlers": ["relay"],
      "interval": 5
    },
    "memory_metrics": {
      "type": "metric",
      "output_type": "graphite",
      "command": "memory-metrics.rb",
      "subscribers": ["all"],
      "handlers": ["relay"],
      "interval": 5
    }
  }
}
